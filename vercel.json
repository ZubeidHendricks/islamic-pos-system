{
    "buildCommand": "dotnet publish -c Release -o publish",
    "outputDirectory": "publish/wwwroot",
    "framework": "blazor",
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
        "API_URL": "https://api.tijaarat.com"
    }
}